# Application
APP_NAME=IroBot
APP_ENV=development
DEBUG=True
LOG_LEVEL=DEBUG

# Database
DATABASE_URL=postgresql://irobot_user:irobot_pass_dev@postgres:5432/irobot_dev

# Redis
REDIS_URL=redis://redis:6379/0

# Weaviate
WEAVIATE_URL=http://weaviate:8080

# JWT
SECRET_KEY=dev_secret_key_change_in_production_32_chars_minimum
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Mistral API
MISTRAL_API_KEY=your_mistral_api_key_here

# Exchange Rate API
EXCHANGE_RATE_API_KEY=your_exchange_rate_api_key_here
EXCHANGE_RATE_API_URL=https://api.exchangerate-api.com/v4/latest/USD

# CORS
CORS_ORIGINS=http://localhost,http://localhost:80,http://localhost:5173

# Rate Limiting
RATE_LIMIT_PER_MINUTE=50
RATE_LIMIT_PER_HOUR=60

# File Upload
MAX_FILE_SIZE_MB=50
MAX_BATCH_SIZE_MB=500
ALLOWED_FILE_TYPES=pdf,docx,doc,xlsx,xls,pptx,ppt,rtf,txt,md,png,jpg,jpeg,webp

# Celery
CELERY_BROKER_URL=redis://redis:6379/0
CELERY_RESULT_BACKEND=redis://redis:6379/0
CELERY_TASK_TRACK_STARTED=True
CELERY_TASK_TIME_LIMIT=3600

# Workers Concurrency (DEV)
WORKER_PROCESSING_CONCURRENCY=2
WORKER_CHUNKING_CONCURRENCY=2
WORKER_EMBEDDING_CONCURRENCY=2
WORKER_INDEXING_CONCURRENCY=2

# Phoenix Tracing
PHOENIX_URL=http://phoenix:6006

# Grafana credentials
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=your_secure_password_here

# Phoenix (optionnel)
PHOENIX_SQL_DATABASE_URL=sqlite:////data/phoenix.db
